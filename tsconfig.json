{
  "compilerOptions": {
    "allowJs": false,
    "noEmit": true,
    "target": "esnext",
    "lib": ["es6"],
    "jsx": "react-native",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "src/*": ["src/*"],
      "src": ["src"]
    },
    "resolveJsonModule": true
  }
}
