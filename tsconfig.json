{
  "compilerOptions": {
    "allowJs": false,
    "noEmit": true,
    "target": "esnext",
    "lib": ["es6"],
    "jsx": "react-native",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "assets": ["./assets"],
      "assets/*": ["./assets/*"],
      "constants": ["./constants"],
      "constants/*": ["./constants/*"],
      "screens": ["./screens"],
      "screens/*": ["./screens/*"],
      "components": ["./components"],
      "components/*": ["./components/*"]
    },
    "resolveJsonModule": true
  }
}
